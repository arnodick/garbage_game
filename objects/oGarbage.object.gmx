          <argument>
            <kind>1</kind>
            <string>///GARBAGE
if ((speed &gt; 14) or (raccoon_picked_up == true))
{
    garbage_chance = garbage_chance_speed;
    if ( !audio_is_playing(sound[0]) and !audio_is_playing(sound[1]) and !audio_is_playing(sound[2]) )
    {
        audio_play_sound(sound[irandom(sound_amount)], 1, false);
    }
}
else
{
    garbage_chance = garbage_chance_init;
}

if (global.level_count == 3)
{
    if (global.garbage_fountain_timer != 0)
    {
        global.garbage_fountain_timer--;
    }
}

if (random(1) &lt; garbage_chance)
{
    if (global.level_count != 3)
    {
        if ( ((global.games_count mod 3) == 0) and (global.plus_mode == true) and instance_exists(oRaccoon_assault))
        {
            var garbage = instance_create(x, y, choose(oCoin, oShard_special));
        }
        else
        {
            var garbage = instance_create(x, y, irandom_range(oMouse, oTampon));
        }
        //irandom_range(oMouse, oTampon)
        with (garbage)
        {
            speed = random_range(8, 13);
            direction = oGarbage.image_angle + random_range(210, 262);
        }
        speed = garbage.speed;
        direction = -garbage.direction;
    }
    else if (global.garbage_fountain_timer &lt;= 0)
    {
        if ( ((global.games_count mod 2) == 0) and (global.plus_mode == true) )
        {
            //BONUS ROUND
            if (!instance_exists(oBonus))
            {
                instance_create(room_width/2, room_height/2, oBonus);
            }
        }
        else if (instance_number(oGarbage) &lt; 160)
        {
            var garbage = instance_create(x, y, oGarbage);
            //irandom_range(oMouse, oTampon)
            with (garbage)
            {
                speed = random_range(8, 13);
                direction = oGarbage.image_angle + random_range(210, 262);
            }
            speed = garbage.speed;
            direction = -garbage.direction;
        }
        else
        {
            global.level_count++;
            with (all)
            {
                instance_destroy();
                room_goto(rPause);
                //room_goto(rAsleep);
            }
        }
    }
}

///Controller
if (picked_up == true)
{
    speed = (point_distance(mouse_x, mouse_y, x, y)/2);
    move_towards_point(mouse_x, mouse_y, speed);
    image_angle = 0;
}
if (instance_exists(oRaccoon))
{
    if (raccoon_picked_up == true)
    {
        speed = (point_distance(oRaccoon.x, oRaccoon.y, x, y)/2);
        move_towards_point(oRaccoon.x, oRaccoon.y, speed);
        image_angle = 0;
    }
}
else
{
    raccoon_picked_up = false;
}
if (speed &gt; 0)
{
    speed -= 1;
    if (speed &lt; 0)
    {
        speed = 0;
    }
    else if (speed &gt; 18)
    {
        speed = 18;
    }
    image_angle -= hspeed*3;
}
if (x &lt; global.room_buffer)
{
    x = global.room_buffer;
}
if (y &lt; global.room_buffer)
{
    y = global.room_buffer;
}
if (x &gt; room_width - global.room_buffer)
{
    x = room_width - global.room_buffer;
}
if (y &gt; room_height - global.room_buffer)
{
    y = room_height - global.room_buffer;
}
